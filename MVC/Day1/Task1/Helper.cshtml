@{
    var chartData = new Chart(width: 600, height: 400)
        .AddTitle("Products Ranges")
        .AddSeries(
            name: "Sales",
            chartType: "Bar",
            xValue: new[] { "North", "South", "East", "West" },
            yValues: new[] { "25000", "10000", "15000", "20000" })
        .GetBytes("png");
}

<img src="data:image/png;base64,@Convert.ToBase64String(chartData)" />




