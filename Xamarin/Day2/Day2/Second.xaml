<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
   xmlns:cimg="clr-namespace:CommunityToolkit.Maui.Views;assembly=CommunityToolkit.Maui" 
             
             x:Class="Day2.Second"
            >
    <AbsoluteLayout>
    <BoxView
        AbsoluteLayout.LayoutBounds="0,0,1,0.5"
        AbsoluteLayout.LayoutFlags="All"
        Color="#062743"
        VerticalOptions="StartAndExpand"
        HorizontalOptions="Center"
        CornerRadius="0,0,100,100"></BoxView>
        <VerticalStackLayout AbsoluteLayout.LayoutBounds="0,0,1,1" AbsoluteLayout.LayoutFlags="All" Padding="5">
            <Label Text="Pizaario."  TextColor="Orange" FontSize="40" FontFamily="bold"></Label>
            <Label Text="Pizaario for online orders ."   TextColor="White" FontSize="20" FontFamily="bold"></Label>
        </VerticalStackLayout>
        <HorizontalStackLayout AbsoluteLayout.LayoutBounds="0,1,1,.87" AbsoluteLayout.LayoutFlags="All" Spacing="10">
            <Label Text="Pizaario"   TextColor="White" FontSize="20" FontFamily="bold"></Label>
            <Label Text="Salad"   TextColor="Gray" FontSize="20" FontFamily="bold"></Label>
            <Label Text="Burger"   TextColor="Gray" FontSize="20" FontFamily="bold"></Label>
            <Label Text="Rice"   TextColor="Gray" FontSize="20" FontFamily="bold"></Label>
        </HorizontalStackLayout>
        <HorizontalStackLayout AbsoluteLayout.LayoutBounds="280,100" AbsoluteLayout.LayoutFlags="None" Spacing="30">
        <Image Source="search.svg" HeightRequest="30" WidthRequest="30" />
        <Image Source="cart.svg" HeightRequest="30" WidthRequest="30"/>
        </HorizontalStackLayout>

        <HorizontalStackLayout Spacing="15" AbsoluteLayout.LayoutBounds="80,150" AbsoluteLayout.LayoutFlags="None" Padding="10">
            <Image Source="next.svg" HeightRequest="40" WidthRequest="30"/>
            <cimg:AvatarView
                ImageSource="data:image/jpeg;base64,"
                HeightRequest="100"
                WidthRequest="100"
                CornerRadius="70"
                />
            <Image Source="pre.svg" HeightRequest="40" WidthRequest="30"/>
        </HorizontalStackLayout>
        <VerticalStackLayout AbsoluteLayout.LayoutBounds="120,250" AbsoluteLayout.LayoutFlags="None" Padding="8">
            <Label Text="    Pizaario"   TextColor="Orange" FontSize="20" FontFamily="bold"></Label>
            <Label Text="Pizaa Chicken"   TextColor="White" FontSize="20" FontFamily="bold"></Label>
            <Label Text="    50 EGP"   TextColor="Orange" FontSize="20" FontFamily="bold"></Label>

        </VerticalStackLayout>
        <VerticalStackLayout AbsoluteLayout.LayoutBounds="10,370" AbsoluteLayout.LayoutFlags="None">
            <Label Text="Popular Today" FontSize="20" TextColor="Black" FontFamily="bold"></Label>
        </VerticalStackLayout>
        <ScrollView AbsoluteLayout.LayoutBounds="10,400" AbsoluteLayout.LayoutFlags="None">
            <StackLayout Orientation="Horizontal">
            <Border
	            Stroke="Orange"
	            Background="white"
	            StrokeThickness="2"
	            Padding="30,20"
	            HorizontalOptions="Center">
                    <VerticalStackLayout>
                        <Image 
                            Source="data:image/jpeg;base64,"
                            WidthRequest="200"
                            HeightRequest="200"
                            />
                        <Label Text="Pizaa Chicken"></Label>
                    </VerticalStackLayout>
            </Border>
                <Border
	            Stroke="Orange"
	            Background="white"
	            StrokeThickness="2"
	            Padding="30,20"
	            HorizontalOptions="Center">
                    <VerticalStackLayout>
                        <Image 
                            Source="data:image/jpeg;base64,"
                            WidthRequest="200"
                            HeightRequest="200"
                            />
                        <Label Text="Pizaa Chicken"></Label>
                    </VerticalStackLayout>
                </Border>

                <Border
	            Stroke="Orange"
	            Background="white"
	            StrokeThickness="2"
	            Padding="30,20"
	            HorizontalOptions="Center">
                    <VerticalStackLayout>
                        <Image 
                            Source="data:image/jpeg;base64,"
                            WidthRequest="200"
                            HeightRequest="200"
                            />
                        <Label Text="Pizaa Chicken"></Label>
                    </VerticalStackLayout>
                </Border>


            </StackLayout>
        </ScrollView>
    </AbsoluteLayout>



</ContentPage>